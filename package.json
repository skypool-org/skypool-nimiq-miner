    "minimist": "^1.2.0",
    "uws": "^9.14.0"
  },
  "bugs": {
    "url": "https://github.com/skypool-org/skypool-nimiq-miner/issues"
  },
  "homepage": "https://github.com/skypool-org/skypool-nimiq-miner#readme"
